html
  head
    title= title
    script(type='text/javascript', src='/javascripts/extern/jquery.min.js')
    script(type='text/javascript', src='/javascripts/extern/underscore-min.js')
    script(type='text/javascript', src='/javascripts/extern/backbone.js')
    script(type='text/javascript', src="/socket.io/socket.io.js")

    script(type='text/javascript', src="/javascripts/math.js")
    script(type='text/javascript', src="/javascripts/input.js")
    script(type='text/javascript', src="/javascripts/collide.js")
    script(type='text/javascript', src="/javascripts/entity.js")
    script(type='text/javascript', src="/javascripts/simulation.js")
    script(type='text/javascript', src="/javascripts/render.js")
    script(type='text/javascript', src="/javascripts/client.js")

    link(rel='stylesheet', href='/stylesheets/style.css')
  body
    header
      span.name Player:#{Math.round(Math.random() * 0xFFFFFFFF).toString(16)}
      span.fps 
      button.start enter game
    .main
      canvas(width=640,height=480)
    .debug
      ul
        li
          input.debug#debug_quadtrees(name='debug_quadtrees', value='quadtrees', type='checkbox')
          label(for='debug_quadtrees') QuadTrees
        li
          input.debug#debug_collisions(name='debug_collisions', value='collisions', type='checkbox')
          label(for='debug_collisions') Collisions
    footer
